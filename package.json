{
  "name": "ts-drizzle-gql",
  "version": "0.1.0",
  "description": "Starter template for Typescript, Drizzle ORM and GraphQL using ExpressJS",
  "main": "index.js",
  "scripts": {
    "build": "shx rm -rf ./dist && tsc --build tsconfig.prod.json && tsc-alias && shx cp -r ./src/core/infrastructure/database/migrations/*.sql ./dist/core/infrastructure/database/migrations",
    "start": "node ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "start:staging": "node ./dist --env staging",
    "db:generate-migrations": "drizzle-kit generate --config=src/core/infrastructure/database/migrations",
    "lint": "eslint '*/**/*.{js,ts,tsx}' .",
    "lintfix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix ."
  },
  "keywords": [
    "typescript",
    "drizzle",
    "postgres",
    "graphql",
    "express",
    "ddd architecture"
  ],
  "author": "Isaac Oluwatemilorun <mail@temilorun.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.11",
    "@types/request-ip": "^0.0.41",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "awilix": "^12.0.4",
    "command-line-args": "^6.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-graphql": "^0.8.5",
    "drizzle-orm": "^0.39.2",
    "express": "^4.21.2",
    "express-ping": "^1.4.0",
    "glob": "7.1.6",
    "graphql": "^16.10.0",
    "graphql-query-complexity": "^1.0.0",
    "graphql-scalars": "^1.24.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "rate-limiter-flexible": "^5.0.5",
    "request-ip": "^3.3.0",
    "ulid": "^2.3.0",
    "winston": "^3.17.0"
  }
}
